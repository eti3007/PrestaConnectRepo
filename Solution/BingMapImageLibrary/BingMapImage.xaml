<UserControl x:Class="BingMapImageLibrary.BingMapImage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Width="500" Height="500">
    
    <Grid x:Name="LayoutRoot" Background="White" Width="400" Height="400">        
        <WebBrowser Source="{Binding Screen.LocalisationProperty.UrlBingMapImage}"></WebBrowser>
    </Grid>
</UserControl>
