              <ScreenExpressionTree>
                <ChainExpression>
                  <MemberExpression
                    Member="CreateNewLocalisation/Properties[LocalisationProperty]" />
                </ChainExpression>
              </ScreenExpressionTree>
            </ContentItem.DataSource>
            <ContentItem
